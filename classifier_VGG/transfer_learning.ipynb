   "outputs": [],
   "source": [
    "model1 = keras.models.Model(inputs=model1.input,outputs=model1.layers[-2].output) # amputate last dense layer\n",
    "new_output = model1.output # take the output as currently defined\n",
    "new_output = keras.layers.Dense(2,activation='softmax')(new_output) # operate on that output with another dense layer\n",
    "model1 = keras.models.Model(inputs=model1.input,outputs=new_output) # define a new model with the new output"
   ]
